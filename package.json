{
   "name": "websocket-pubsub",
   "version": "0.0.33",
   "description": "Solução de PubSub & RPC usando WebSocket e node.js para aplicações de baixa e média complexidade",
   "main": "dist/node.js",
   "repository": {
      "type": "git",
      "url": "git+https://github.com/nidorx/websocket-pubsub.git"
   },
   "files": [
      "dist",
      "LICENSE",
      "README.md"
   ],
   "scripts": {
      "test": "jest --runInBand",
      "build-ts": "tsc",
      "build": "npm run build-ts && npm run test"
   },
   "author": "Alex Rodin <contato@alexrodin.info>",
   "license": "MIT",
   "dependencies": {
      "stoppable": "1.1.0"
   },
   "devDependencies": {
      "@types/ws": "6.0.1",
      "@types/jest": "23.3.14",
      "@types/express": "4.16.1",
      "@types/react-native": "0.57.34",
      "ws": "6.1.3",
      "jest": "23.6.0",
      "rimraf": "2.6.3",
      "express": "4.16.4",
      "ts-jest": "23.10.5",
      "typescript": "3.3.3"
   },
   "jest": {
      "bail": true,
      "notify": false,
      "verbose": true,
      "transform": {
         "^.+\\.tsx?$": "ts-jest"
      },
      "setupTestFrameworkScriptFile": "<rootDir>/tests/test-setup.ts",
      "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?|ts?)$",
      "testEnvironment": "node",
      "moduleFileExtensions": [
         "ts",
         "tsx",
         "js",
         "jsx",
         "json",
         "node"
      ],
      "collectCoverage": true,
      "coverageReporters": [
         "text",
         "text-summary",
         "json",
         "lcov",
         "html"
      ],
      "testPathIgnorePatterns": [
         "/node_modules/",
         "/dist",
         "/example",
         "/doc",
         "/src"
      ]
   },
   "peerDependencies": {
      "ws": "^6.1.3"
   }
}