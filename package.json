{
   "name": "websocket-pubsub",
   "version": "0.0.23",
   "description": "Solução de PubSub & RPC usando WebSocket e node.js para aplicações de baixa e média complexidade",
   "main": "dist/node.js",
   "files": [
      "dist",
      "LICENSE",
      "README.md"
   ],
   "scripts": {
      "test": "jest",
      "build": "npm run test && npm run build-ts",
      "build-ts": "tsc"
   },
   "author": "Alex Rodin <contato@alexrodin.info>",
   "license": "MIT",
   "dependencies": {},
   "devDependencies": {
      "@types/express": "^4.16.0",
      "@types/jest": "^23.3.1",
      "@types/react-native": "0.55.23",
      "@types/ws": "^5.1.2",
      "express": "^4.16.3",
      "jest": "^23.4.2",
      "rimraf": "^2.6.3",
      "ts-jest": "^23.0.1",
      "typescript": "^2.9.2",
      "ws": "^5.2.1"
   },
   "jest": {
      "bail": true,
      "notify": true,
      "verbose": false,
      "transform": {
         "^.+\\.tsx?$": "ts-jest"
      },
      "setupTestFrameworkScriptFile": "<rootDir>/tests/test-setup.ts",
      "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?|ts?)$",
      "testEnvironment": "node",
      "moduleFileExtensions": [
         "ts",
         "tsx",
         "js",
         "jsx",
         "json",
         "node"
      ],
      "collectCoverage": true,
      "coverageReporters": [
         "text",
         "text-summary",
         "json",
         "lcov",
         "html"
      ],
      "testPathIgnorePatterns": [
         "/node_modules/",
         "/dist",
         "/example",
         "/doc",
         "/src"
      ]
   },
   "peerDependencies": {
      "ws": "^5.2.1"
   }
}