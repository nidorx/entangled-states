{
   "name": "websocket-pubsub",
   "version": "0.0.1",
   "description": "PubSub para aplicações de baixa e média complexidade node.js + websocket",
   "main": "dist/index.js",
   "scripts": {
      "start": "npm run watch",
      "build": "npm run build-ts",
      "serve": "node dist/index.js",
      "build-ts": "tsc",
      "watch-ts": "tsc -w",
      "watch-node": "nodemon dist/index.js",
      "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\""
   },
   "author": "Alex Rodin <contato@alexrodin.info>",
   "license": "MIT",
   "dependencies": {
      "dffptch": "^1.0.4"
   },
   "devDependencies": {
      "ws": "^5.2.1",
      "@types/ws": "^5.1.2",
      "concurrently": "^3.5.1",
      "nodemon": "^1.13.0",
      "typescript": "^2.9.2"
   },
   "peerDependencies": {
      "ws": "^5.2.1"
   }
}